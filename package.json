{
  "name": "running-dinner-tool",
  "version": "2.3.2",
  "homepage": "./",
  "main": "src/electron-starter.js",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "date-fns": "^2.10.0",
    "electron": "^8.0.1",
    "electron-log": "^4.0.7",
    "mapbox-gl": "^1.8.0",
    "papaparse": "^5.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "websocket": "^1.0.31"
  },
  "author": {
    "name": "Jonathan Kloss"
  },
  "description": "Running Dinner Tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-kloss/Running-Dinner-Tool"
  },
  "build": {
    "appId": "running.dinner.tool",
    "productName": "Running Dinner Tool",
    "publish": [
      {
        "provider": "github",
        "owner": "john-kloss",
        "repo": "Running-Dinner-Tool"
      }
    ],
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ],
      "icon": "assets/dinner.ico"
    },
    "mac": {
      "target": "default",
      "icon": "assets/dinner.icns"
    },
    "linux": {
      "target": "AppImage",
      "publish": [
        "github"
      ],
      "icon": "assets/dinner.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "devDependencies": {
    "electron-builder": "^22.3.2",
    "@testing-library/react": "^9.4.1"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "electron": "electron .",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "publish": " electron-builder -wl -p always",
    "mac": "electron-builder --mac",
    "win": "electron-builder --win",
    "linux": "electron-builder --linux",
    "build": "npm run react-build && electron-builder -wl -p always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
